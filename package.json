  "scripts": {
    "env:dev": "cross-env SHELL=bash MHW_ENV=development NODE_ENV=development",
    "env:test": "cross-env SHELL=bash MHW_ENV=test NODE_ENV=production",
    "env:electron": "cross-env IS_ELECTRON=true",
    "build": "cross-env-shell 'vue-cli-service build'",
    "test:unit": "run-s env:test _test:unit",
    "test:e2e": "run-s env:test _test:e2e:ci",
    "lint": "run-s env:dev tsc lint:css _lint:ts",
    "_icons": "cross-env-shell 'electron-icon-builder --input=./public/android-chrome-512x512.png --output dist/electron --flatten'",
    "_lint:ts": "cross-env-shell 'vue-cli-service lint --fix \"src/**/*.vue\" \"src/**/*.ts\"'",
    "_test:e2e:dev": "cross-env-shell 'vue-cli-service test:e2e'",
    "_test:e2e:ci": "cross-env-shell 'vue-cli-service test:e2e --headless'",
    "_test:unit": "cross-env-shell 'vue-cli-service test:unit --env jsdom'",
    "_test:unit:u": "cross-env-shell 'vue-cli-service test:unit -u'",
    "_build:electron": "cross-env-shell 'vue-cli-service build:electron'",
    "build:electron": "run-s env:electron _icons _build:electron",
    "ci": "run-s env:test _test:unit _test:e2e:ci",
    "clean": "rm -rf ./node_modules/.cache && rm -rf dist && mkdir -p dist/web && mkdir -p dist/electron",
    "deptree": "depcruise --exclude '(node_modules|test|tests|__tests__|dist|assets|components)' --output-type dot src | dot -T svg > src/ui/assets/deps.svg",
    "_dev": "cross-env-shell 'vue-cli-service serve'",
    "dev": "run-s env:dev _dev",
    "_dev:electron": "cross-env-shell 'vue-cli-service electron:serve'",
    "dev:electron": "run-s env:dev env:electron _dev:electron",
    "lint:css": "cross-env-shell 'stylelint --fix \"src/**/*.vue\" \"src/**/*.css\"'",
    "lint:ts": "run-s _lint:ts",
    "stats": "cross-env-shell 'webpack --profile --json > webpack-stats.json'",
    "test": "run-s env:test _test:unit _test:e2e:ci",
    "test:e2e:dev": "run-s env:test _test:e2e:dev",
    "test:unit:u": "run-s env:test _test:unit:u",
    "tsc": "cross-env-shell 'tsc --noEmit'",
    "cypress": "cypress",
    "vue": "vue",
    "vue-cli-service": "vue-cli-service",
    "vue-i18n-extract": "vue-i18n-extract",
    "vue-i18n-extract:report": "vue-i18n-extract report -v 'src/**/*.?(ts|vue)' -l 'src/lang/*.?(json)'"
