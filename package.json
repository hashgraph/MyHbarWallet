  "private": true,
  "scripts": {
    "cy:run": "cypress run",
    "dev": "vite",
    "electron:dev": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' electron -r ts-node/register src/electron/main.ts",
    "prebuild": "test \"$CI\" = true && npx pnpm install --store=node_modules/.pnpm-store || true",
    "build": "cross-env NODE_ENV=production vite build",
    "fmt": "prettier -w .",
    "postinstall": "husky install",
    "check": "run-s check:fmt check:types check:eslint",
    "check:fmt": "prettier -c .",
    "check:eslint": "eslint --ext .js,.ts,.vue src",
    "check:types": "run-s check:types:src check:types:tests:cypress",
    "check:types:src": "vue-tsc",
    "check:types:tests:cypress": "tsc -p tests/cypress"
  },
  "dependencies": {
