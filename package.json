    "scripts": {
        "_build": "cross-env NODE_ENV=production vue-cli-service build",
        "_electron:build": "cross-env SHELL=bash NODE_ENV=production vue-cli-service electron:build",
        "_electron:serve": "vue-cli-service electron:serve",
        "_lint:js": "vue-cli-service lint --fix 'src/**/*.vue' 'src/**/*.ts'",
        "_serve": "vue-cli-service serve",
        "_test:e2e": "vue-cli-service test:e2e",
        "_test:unit": "jest --clearCache && vue-cli-service test:unit",
        "build": "npm-run-all deptree config:front _build config:rm",
        "build:clean": "rm -rf dist/ && mkdir -p dist/electron && mkdir -p dist/web",
        "ci": "npm-run-all lint config:front _test:unit _build config:rm",
        "config:back": "cp vue.config.back.js vue.config.js",
        "config:front": "cp vue.config.front.js vue.config.js",
        "config:rm": "rm vue.config.js",
        "_stats": "webpack --profile --json > webpack-stats.json",
        "stats": "npm-run-all config:front _stats config:rm",
        "deptree": "depcruise --exclude '(node_modules|test|tests|__tests__|dist|assets)' --output-type dot src | dot -T svg > src/ui/assets/deps.svg",
        "electron:build": "npm-run-all deptree config:back electron:generate-icons _electron:build config:rm",
        "electron:generate-icons": "electron-icon-builder --input=./public/android-chrome-512x512.png --output=dist/electron --flatten",
        "electron:serve": "npm-run-all config:back _electron:serve config:rm",
        "lint": "npm-run-all tsc lint:js lint:css",
        "lint:css": "stylelint --fix 'src/**/*.vue' 'src/**/*.css'",
        "lint:js": "npm-run-all config:front _lint:js config:rm",
        "serve": "npm-run-all config:front _serve config:rm",
        "test:unit:compare": "bash -c 'comm <(basename -a -s.vue src/ui/components/*.vue | sort) <(basename -a -s.spec.ts tests/unit/components/*.spec.ts | sort)'",
        "test": "npm-run-all config:back _test:unit config:rm",
        "test:e2e": "npm-run-all config:back _test:e2e config:rm",
        "test:unit": "npm-run-all config:front _test:unit config:rm",
        "tsc": "tsc --noEmit",
        "vue-i18n-extract": "vue-i18n-extract",
        "vue-i18n-extract:report": "vue-i18n-extract report -v 'src/**/*.?(ts|vue)' -l 'src/lang/*.?(json)'"
